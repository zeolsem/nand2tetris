/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
      Nand(a=a, b=b, out=nandAB);
      Nand(a=a, b=nandAB, out=aNandAB);
      Nand(a=b, b=nandAB, out=bNandAB);
      Nand(a=aNandAB, b=bNandAB, out=out);
}
